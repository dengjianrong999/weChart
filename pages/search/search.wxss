
.g-doc, page {
  width: 100%;
  height: 100%;
  position: relative;
  background-color: #fff;
}
 
.g-doc {
  width: 100%;
  height: 100%;
  position: relative;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  flex-flow: column;
}
 
.layer-top {
  height: 50px;
}
 
.layer-bottom {
  height: 50px;
}
 
.g-bd {
  width: 100%;
  background: transparent;
  -webkit-box-flex: 1;
  flex: 1;
}
 
.g-ft {
  width: 100%;
  bottom: 0;
  position: fixed;
}
.listT{
  padding: 30rpx 50rpx;
  border-top:1px solid #ededed;
}
.loa{
  width: 100rpx;
  height: 100rpx;
}
.tet{
  background: #fff;
}
.loading{
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #fff;
  color: #333;
}
.inp-holder{
  width:90%;
  margin: 0 auto;
  height: 42px;
  padding-left: 10rpx;
  border: 1px solid #ededed;
}