/* pages/page2/page2.wxss */
.img {
  width: 200rpx;
  height: 200rpx;
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
}
 .reason {
  padding: 10rpx 30rpx;
}
.reason_item {
  display: inline-block;  
  height: 20rpx;  
  line-height: 20rpx;  
  padding: 20rpx;  
  border: 1rpx solid #80c587;  
  color: #1ecc8b;  
  border-radius: 30rpx;  
  margin-right: 30rpx;  
  margin-bottom: 30rpx;
}
image{
  width: 100%;
  height: 200rpx;
}
.oneTitle{
  width: 100%;
  padding: 10rpx;
  border-bottom: 1px solid #fff;
  background: #ddd;
}
.twoTitle{
  width: 100%;
  padding: 5rpx;
  transition: all .3s;
  border-bottom: 1px solid #ebebeb;
}
.twoTitle:hover{
  background-color: #1ecc8b;
}
.showClass{
  height: auto;
}
.hideClass{
  height: 0;
}